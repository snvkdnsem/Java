
public class Check {

	public static void main(String[] args) {
		// #2
		Math math = new Math();
		
		//double result = Math.round();
		double result = math.round(3.141592);
		
		System.out.println("result = " + result);
	}

}

class Math{
	// #1 static
	double round(double value) {
		return java.lang.Math.round(value);
	}
}